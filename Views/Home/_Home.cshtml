<div class="row">
    <div class="col-md-4"></div>
    <div class="col-md-4">
        <dl class="dl-horizontal" id="lp-status">
            <dt>Data name</dt>
            <dd id="data-name"></dd>
            <dt>Processing count</dt>
            <dd id="proc-count"></dd>
            <dt>Loaded time</dt>
            <dd id="loaded-time"></dd>
            <dt>Prossing time</dt>
            <dd id="processed-time"></dd>
        </dl>
    </div>
    <div class="col-md-4"></div>
</div>
<div class="row">
    <div class="col-md-4"></div>
    <div class="col-md-4">
        <div class="drop-box-outer">
            <div class="drop-box">
                <form class="drop-target upload-form">
                    <div class="drop-target">
                        <label for="file-selector">Select files</label> or drop file here.
                        <input name="files[]" type="file" id="file-selector"/>
                    </div>
                    <div class="upload-drop-zone drop-target">
                        <svg class="down-arrow-box-icon" width="50" height="43" viewBox="0 0 50 43"><path d="M48.4 26.5c-.9 0-1.7.7-1.7 1.7v11.6h-43.3v-11.6c0-.9-.7-1.7-1.7-1.7s-1.7.7-1.7 1.7v13.2c0 .9.7 1.7 1.7 1.7h46.7c.9 0 1.7-.7 1.7-1.7v-13.2c0-1-.7-1.7-1.7-1.7zm-24.5 6.1c.3.3.8.5 1.2.5.4 0 .9-.2 1.2-.5l10-11.6c.7-.7.7-1.7 0-2.4s-1.7-.7-2.4 0l-7.1 8.3v-25.3c0-.9-.7-1.7-1.7-1.7s-1.7.7-1.7 1.7v25.3l-7.1-8.3c-.7-.7-1.7-.7-2.4 0s-.7 1.7 0 2.4l10 11.6z" /></svg>
                    </div>
                    <div class="row">
                        <div class="col-md-4 col-md-push-4">
                            <input type="submit" class="btn btn-primary" value="Upload" disabled />
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div class="col-md-4"></div>
    </div>
</div>
